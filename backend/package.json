{
  "name": "biohax-backend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc --project tsconfig.build.json",
    "start": "node dist/server.js",
    "lint": "node scripts/run-eslint.cjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:migrate": "prisma migrate dev",
    "db:migrate": "tsx scripts/run-migrate.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:reset": "prisma migrate reset --force --skip-generate --skip-seed --schema prisma/schema.prisma && tsx scripts/seed.ts",
    "whoop:daemon": "node dist/scripts/whoop-sync-daemon.js",
    "perf:api": "tsx scripts/run-perf.ts",
    "workers:run": "node dist/src/workers/runner.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.3",
    "@prisma/client": "^5.21.1",
    "@prisma/engines": "^6.19.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "redis": "^5.9.0",
    "resend": "^6.4.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^14.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.7",
    "@types/node": "^22.10.1",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "embedded-postgres": "^18.0.0-beta.15",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "prisma": "^5.21.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "yaml": "^2.8.1"
  }
}
