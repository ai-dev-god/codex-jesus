# Copy to devops/release.env and replace placeholder secrets before running devops/release.sh

POSTGRES_DB=biohax
POSTGRES_USER=biohax
POSTGRES_PASSWORD=biohax
POSTGRES_PORT=6543

# Derived connection string used by backend and Prisma migrations.
DATABASE_URL=postgresql://biohax:biohax@postgres:5432/biohax?schema=public

# Service image repositories; devops/release.sh will append the release tag.
BACKEND_IMAGE=biohax-backend
FRONTEND_IMAGE=biohax-frontend

# Public endpoints exposed to end-to-end tests and smoke scripts.
BACKEND_PORT=4000
FRONTEND_PORT=8080
CORS_ORIGIN=http://localhost:8080
WHOOP_REDIRECT_URI=http://localhost:8080/oauth/whoop/callback

# Secrets required for authentication and integrations.
AUTH_JWT_SECRET=change-me-to-a-long-secret
AUTH_REFRESH_ENCRYPTION_KEY=change-me-to-a-secure-key
WHOOP_TOKEN_ENCRYPTION_KEY=change-me-to-a-long-secret
WHOOP_TOKEN_KEY_ID=whoop-token-key-v1
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_PLANNER_MODEL=openrouter/openai/gpt-4.1-mini
OPENROUTER_SAFETY_MODEL=openrouter/google/gemini-2.0-flash
OPENROUTER_NUMERIC_MODEL=openrouter/deepseek/deepseek-r1-distill-qwen-32b
WHOOP_CLIENT_ID=
WHOOP_CLIENT_SECRET=
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
RESEND_API_KEY=
WORKER_QUEUES=insights-generate,whoop-sync,notifications-dispatch,longevity-plan-generate
WORKER_POLL_INTERVAL_MS=5000
WORKER_ERROR_BACKOFF_MS=2000

# Optional overrides for frontend asset compilation.
VITE_API_BASE_URL=http://backend:4000

# Optional overrides for deterministic seed data.
SEED_MEMBER_EMAIL=member@example.com
SEED_MEMBER_PASSWORD=PlaywrightSeedPass1!
SEED_ADMIN_EMAIL=admin@example.com
SEED_ADMIN_PASSWORD=PlaywrightAdminPass1!
